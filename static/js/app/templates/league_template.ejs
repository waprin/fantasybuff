    <% if (loaded) { %>
    <li>
        <a href="/leagues/espn/<%=espn_id %>/<%=year %>/"><%=name %> <%=year %>  </a>
            Last Synchronized: <%= lineups_scrape_finish_time %> <div class="calculated">Last Calculated <%= league_loaded_finish_time %></div>
    </li>

    <% } else if (failed) { %>
       <li class="failed">Failed: <%=name %> <%=year %></li>
    <% } else if (!loading) { %>
       <li class="queued">Queued To Load: <%=name %> <%=year %></li>
    <% } else { %>
            <% if (year === '2014') { %>
              <li>Loading: <%=name %> <%=year %>  Scraped: <%= pages_scraped %> Total: <%= total_pages %></li>
              <div class="progress">
                  <div class="progress-bar" style="width: <%= percent_done %>%;"> </div>
              </div>
            <% } else { %>
              <li>Loading: <%=name %> <%=year %></li>
              <img class="league_loading_spinner" src="/static/ajax-loader.gif"/>
              Legacy leagues can take 5-10 minutes to load
           <% } %>
    <% } %>
